{
  "kind": "collectionType",
  "collectionName": "blog_posts",
  "info": {
    "name": "Blog post"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Title": {
      "type": "string",
      "unique": true,
      "required": true
    },
    "Content": {
      "type": "richtext"
    },
    "Date": {
      "type": "datetime"
    },
    "Author": {
      "model": "author"
    },
    "categories": {
      "via": "blog_posts",
      "collection": "category",
      "dominant": true
    },
    "Slug": {
      "type": "string",
      "required": true,
      "unique": true
    }
  }
}
